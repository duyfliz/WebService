package org.vgu.sql;

import java.util.ArrayList;
import java.util.List;

public class SQLColumn {
	private String name;
	private String type;
	private List<String> prpertie = new ArrayList<String>();
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getType() {
		return type;
	}
	public void setType(String type) {
		this.type = type;
	}
	public List<String> getPrpertie() {
		return prpertie;
	}
	public void setPrpertie(List<String> prpertie) {
		this.prpertie = prpertie;
	}
	
	
}
